    "\n",
    "\n",
    "def contour(f, c, xs=[0.0, 1.0], ys=[0.0, 1.0], delta=0.01):\n",
    "    curves = []\n",
    "    nx, ny = len(xs), len(ys)\n",
    "    for i in range(nx - 1):\n",
    "        for j in range(ny - 1):\n",
    "            xmin, xmax = xs[i], xs[i + 1]\n",
    "            ymin, ymax = ys[j], ys[j + 1]\n",
    "\n",
    "            def f_cell(x, y):\n",
    "                return f(xmin + (xmax - xmin) * x, ymin + (ymax - ymin) * y)\n",
    "\n",
    "            done = set()\n",
    "            for n in [0, 1, 2, 3]:\n",
    "                if n not in done:\n",
    "                    rotated_f_cell = rotate_function(f_cell, n)\n",
    "                    x_curve_r, y_curve_r = simple_contour2(rotated_f_cell, c, delta)\n",
    "                    exit = None\n",
    "                    if len(x_curve_r) >= 1:\n",
    "                        xf, yf = x_curve_r[-1], y_curve_r[-1]\n",
    "                        if xf == 0.0:\n",
    "                            exit = LEFT\n",
    "                        elif xf == 1.0:\n",
    "                            exit = RIGHT\n",
    "                        elif yf == 0.0:\n",
    "                            exit = DOWN\n",
    "                        elif yf == 1.0:\n",
    "                            exit = UP\n",
    "                    if exit is not None:  # a fully successful contour fragment\n",
    "                        exit = rotate_direction(exit, n)\n",
    "                        done.add(exit)\n",
    "\n",
    "                    x_curve, y_curve = [], []\n",
    "                    for x_r, y_r in zip(x_curve_r, y_curve_r):\n",
    "                        x, y = rotate(x_r, y_r, n=-n)\n",
    "                        x_curve.append(x)\n",
    "                        y_curve.append(y)\n",
    "                    x_curve = np.array(x_curve)\n",
    "                    y_curve = np.array(y_curve)\n",
    "                    curves.append(\n",
    "                        (xmin + (xmax - xmin) * x_curve, ymin + (ymax - ymin) * y_curve)\n",
    "                    )\n",
    "    return curves\n",
    "\n",
    "#################### Méthode Newton 2D##########################\n",
    "#Cette méthode fut la première envisagée. Cependant la nécessité d'inverser une matrice jacobienne\n",
    "#semble être source d'erreurs. Nous dûmes alors nous reporter sur la méthode présentée précédemment.\n",
    "\n",
    "\n",
    "#Il est nécessaire de définir un problème 2X2 prenant en compte toutes les conditions\n",
    "#voulues. On définit alors la fonction g\n",
    "def g(x,y,a,b,c=0.,delta=0.01):\n",
    "    return np.array([[f(x,y)-c],[(x-a)**2+(b-y)**2-delta**2]])\n",
    "#Et on implémente sa jacobienne\n",
    "\n",
    "def Jacg(x,y,a,b,c=0,delta=0.01):\n",
    "    j= autograd.jacobian\n",
    "    J=np.c_[j(g,0)(x,y,a,b,c,delta),j(g,1)(x,y,a,b,c,delta)]\n",
    "    return J\n",
    "\n",
    "#On généralise alors la méthode de newton en deux dimension en l'appliquant à la fonction g\n",
    "def newton2D(x0,y0,a,b,delta=0.01,c=0.0,eps=2**(-15)):\n",
    "    k=0\n",
    "    kint=40\n",
    "    U=np.array([[x0],[y0]])\n",
    "    V=U-np.linalg.inv(Jacg(float(U[0]),float(U[1]),a,b,c,delta)).dot(np.array([[float(f(U[0],U[1])-c)],[float(g(float(U[0]),float(U[1]),a,b,c,delta)[1])]]))\n",
    "    while (math.sqrt((V[1]-U[1])**2+(V[0]-U[0])**2)>eps and k<kint):\n",
    "        U=V\n",
    "        V=U-np.linalg.inv(Jacg(float(U[0]),float(U[1]),a,b,c,delta)).dot(np.array([[float(f(U[0],U[1])-c)],[float(g(float(U[0]),float(U[1]),a,b,c,delta)[1])]]))\n",
    "        k+=1\n",
    "    if 0<=V[0]<=1 and 0<=V[1]<=1:\n",
    "        return (V[0][0],V[1][0])\n",
    "    else:\n",
    "        return None\n",
    "# Il est également nécessaire de d'indiquer la direction de propagation de Newton2D qui \n",
    "# recquière un point d'initialisaton. On utilise pour cela la fonction ini_newton qui \n",
    "# donne une approximation du point suivant avec l'orthogonal du gradient.\n",
    "def ini_newton(a,b,delta=0.01):\n",
    "    G=grad_f(a,b)\n",
    "    T=np.array([G[1],-G[0]])\n",
    "    T2=delta/(np.linalg.norm(T))*T\n",
    "    return np.array([a+T2[0],b+T2[1]])\n",
