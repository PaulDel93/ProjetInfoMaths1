    "        a=Superfind_seed(p,q,r,s,c)\n",
    "        for i in range(len(a)):\n",
    "            pt=Superfind_seed(p,q,r,s,c)[i]\n",
    "            x.append(pt[0])\n",
    "            y.append(pt[1])\n",
    "            G=grad_f(pt[0],pt[1])\n",
    "            T=np.array([G[1],-G[0]])\n",
    "            T2=delta/(np.linalg.norm(T))*T\n",
    "            while p<pt[0]+T2[0]<q and r<pt[1]+T2[1]<s:\n",
    "                a=pt[0]\n",
    "                b=pt[1]\n",
    "                G=grad_f(a,b)\n",
    "                T=np.array([G[1],-G[0]])\n",
    "                T2=delta/(np.linalg.norm(T))*T\n",
    "                pt=np.array([a+T2[0],b+T2[1]])\n",
    "                x.append(pt[0])\n",
